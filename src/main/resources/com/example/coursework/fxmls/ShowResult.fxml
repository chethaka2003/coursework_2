<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.StackedBarChart?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="background" prefHeight="650.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.ShowResultController">
   <children>
      <StackedBarChart fx:id="barChart" categoryGap="3.0" layoutX="133.0" layoutY="108.0" legendSide="TOP" prefHeight="434.0" prefWidth="634.0" styleClass="barchart" titleSide="LEFT">
        <xAxis>
          <NumberAxis label="Points" side="BOTTOM" tickLabelGap="5.0" upperBound="19.0" />
        </xAxis>
        <yAxis>
          <CategoryAxis label="Projects" side="LEFT" />
        </yAxis>
      </StackedBarChart>
      <ImageView fitHeight="39.0" fitWidth="38.0" layoutX="21.0" layoutY="21.0" onMouseClicked="#backBtn" pickOnBounds="true" preserveRatio="true" styleClass="back">
         <image>
            <Image url="@../images/Adminback.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="54.0" fitWidth="52.0" layoutX="69.0" layoutY="14.0" onMouseClicked="#homeBtn" pickOnBounds="true" preserveRatio="true" styleClass="home">
         <image>
            <Image url="@../images/home.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
